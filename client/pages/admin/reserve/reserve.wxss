page{
  background: #eee;
}
.navs{
  width:100%;
  display: flex;
  justify-content: space-around;
  line-height: 2;
  background: #fff;
  margin-bottom:16rpx;
}
.changes{
  color:#8FC1FF;
  border-bottom:5rpx solid #8FC1FF;
}
.unchanges{
  color:#000;
}
.warp-new{
  width:100%;
  height:160rpx;
  background-color:#ffffff;
  font-size:24rpx;
  display: flex;
  margin: 16rpx auto;
}
.left{
  height:160rpx;
   display: flex;
  flex-direction:column;  
  justify-content: center; 
  margin:auto 28rpx;
}
.content{
  width:100%;
  margin:0 auto;
  display: flex;  
  flex-direction: row;
  justify-content:space-between; 
}
.box-left{
  line-height: 2;
}
.box-right{
  float:right;
  margin-right: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content:space-between;
  
}
.Prompt-new{
  display: flex;
  align-items: center;
  justify-content: center;  
  height:40rpx;
  width:140rpx;
  background-color:#ffe7e7;
  border-radius:20rpx;
  margin:26rpx auto;
  color:#ffabab;
}
.warp-old{
  width:100%;
  height:160rpx;
  background-color:#ffffff;
  font-size:24rpx;
  display: flex;
}
.Prompt-old{
  display: flex;
  align-items: center;
  justify-content: center;  
  height:40rpx;
  width:140rpx;
  background-color:#e8ffe7;
  border-radius:20rpx;
  margin:26rpx auto;
  color:#87e58f;
}
.show{
  display: block;
  background: #fff;
}
.hide{
  display: none;
  visibility: hidden;
}
.dete{
  width:100%;
  background-color:#5DAEF8;
  color:wheat;
  position:fixed;
  bottom:0;
}